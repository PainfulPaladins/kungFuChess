<table id='playerstable' class='table'>
<tr class=''>
    <th>Player</td>
    <th></td>
</tr>

<% foreach my $player (@{$players}){ %>
    <tr>
        <td>
            <%= $player->{rating} %>
        </td>
        <td>
            <%= $player->{screenname} %>
        </td>
    </tr>
<% } %>
</table>
